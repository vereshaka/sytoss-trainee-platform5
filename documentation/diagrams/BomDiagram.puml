@startuml

entity Discipline {
id: long
name: string
}

entity Topic {
id: long
name: string
}

entity Task {
id: long
question: string
answer: string
}

entity Condition {
id: long
name: string
}

entity TaskDomain {
id: long
name: string
script: string
}

entity Test {
id: long
name: string
}

entity PersonalTest {
id: long
name: string
studentId: long
date: date
duration: datetime
attempt: int
answers: List<Task>
}

Task "1" o-- "N conditions" Condition
Task "N tasks" o--o "N topics" Topic
Task "1" o-- "1" TaskDomain
Task "N tasks" --o "1" PersonalTest

Topic "N topics" --o "1" Test

Test "1" --o "N personalTests" PersonalTest

Discipline "1" o-- "N topics" Topic
Discipline "1" o-- "N tests" Test

@enduml